# Overwritted by mvx-api-deployer
NODE_ENV=devnet # mainnet | testnet
PORT=3005
PRIVATE_PORT=4002
CACHEWARMER_PORT=6002
LISTEN_ADDRESS=0.0.0.0
PRIVATE_LISTEN_ADDRESS=0.0.0.0
NODE_CONFIG_DIR="src/config"

# keepalive
KEEPALIVE_TIMEOUT_DOWNSTREAM=45000
KEEPALIVE_TIMEOUT_UPSTREAM=65000

#Redis for cache
REDIS_URL="localhost"
REDIS_PREFIX="development_env"
REDIS_PASSWORD=""
REDIS_PORT=6380
REDIS_DB=0

#MongoDB
MONGODB_URL=mongodb://localhost:27018
MONGODB_DATABASE=development
MONGODB_USERNAME=admin_5151
MONGODB_PASSWORD=admin_5151

#Elasticsearch server to use for logging. Optional
ELASTICSEARCH_URL="https://testnet-index.multiversx.com"

#MultiversX resources
MX_API_URL="http://localhost:3001"
MX_GATEWAY_URL="http://localhost:8079"
MX_DEX_URL="https://devnet-graphql-next.maiar.exchange"
MX_DATA_API_URL="https://data-api.multiversx.com"

#Enable or Disable modules
ENABLE_PUBLIC_API=true
ENABLE_PRIVATE_API=true
ENABLE_CACHE_WARMER=true
ENABLE_EVENTS_NOTIFIER=false
ENABLE_TRACER=false

#Log filename to use for file logging. Optional
LOG_FILE=

#Log level can be one of : error, debug, info
LOG_LEVEL=info

# JWT Authorization
JWT_SECRET=
NATIVE_AUTH_MAX_EXPIRY_SECONDS=86400
NATIVE_AUTH_ACCEPTED_ORIGINS=
IMPERSONATE_URL=

# RabbitMQ
RABBITMQ_URL=
RABBITMQ_EXCHANGE=all_events
RABBITMQ_QUEUE=dex_service

# Other
ENABLE_CACHE_GUEST_RATE_THRESHOLD=100
GUEST_CACHE_REDIS_BATCH_SIZE=3
NATIVE_AUTH_PEM_PATH=

# AWS Timestream
AWS_TIMESTREAM_READ=false
AWS_TIMESTREAM_WRITE=false

TIMESCALEDB_URL="localhost"
TIMESCALEDB_PORT=5432
TIMESCALEDB_DATABASE="postgres"
TIMESCALEDB_USERNAME="postgres_5151"
TIMESCALEDB_PASSWORD="postgres_5151"